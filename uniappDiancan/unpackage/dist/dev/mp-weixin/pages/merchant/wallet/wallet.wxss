page {
  background: #FAFAFA;
}
.Withdrawal {
  width: 100%;
  padding: 24rpx 20rpx;
  box-sizing: border-box;
  background: #FFFFFF;
  border-radius: 20rpx 20rpx 20rpx 20rpx;
  margin: auto;
  margin-bottom: 24rpx;
}
.Withdrawal .withdrawalHistory {
  font-size: 24rpx;
  color: #AAAAAA;
}
.Withdrawal .withdrawalTitle {
  font-weight: 400;
  font-size: 28rpx;
  text-align: center;
  margin-top: 40rpx;
}
.Withdrawal .canWithdrawal {
  margin-top: 20rpx;
  font-weight: bold;
  font-size: 64rpx;
  color: #333333;
  text-align: center;
}
.shouyi {
  padding: 20rpx 60rpx 0;
  box-sizing: border-box;
}
.yield .one {
  font-size: 24rpx;
  color: #777777;
  margin-right: 20rpx;
}
.yield .two {
  font-size: 24rpx;
  color: #E40030;
}
.wallet_box {
  height: 100vh;
  display: flex;
  flex-direction: column;
}
.wallet_box .card {
  background: #F1F2F2;
  padding: 24rpx 0;
  box-sizing: border-box;
  width: 700rpx;
  margin: 0 auto;
  border-radius: 0rpx 0rpx 30rpx 30rpx;
  height: 446rpx;
}
.wallet_box .card .tix {
  width: 130rpx;
  height: 50rpx;
  border-radius: 20rpx 0 0 20rpx;
  position: absolute;
  top: 300rpx;
  right: 20rpx;
  background-color: #fff;
  font-size: 24rpx;
  color: #00ABFF;
  text-align: center;
  line-height: 50rpx;
}
.wallet_box .card .head {
  position: absolute;
  top: 60rpx;
  left: 80rpx;
  color: #fff;
  font-size: 34rpx;
  line-height: 60rpx;
}
.wallet_box .card .head image {
  width: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}
.wallet_box .card .text {
  font-size: 28rpx;
  color: #fff;
  position: absolute;
  top: 290rpx;
  left: 80rpx;
}
.wallet_box .card .money {
  font-size: 50rpx;
  color: #fff;
  position: absolute;
  top: 180rpx;
  left: 80rpx;
}
.wallet_box .card .wellet {
  width: 700rpx;
  height: 350rpx;
}
.wallet_box .detail {
  background-color: #fff;
  border-radius: 40rpx 40rpx 0 0;
  flex-grow: 1;
  overflow: hidden;
}
.wallet_box .detail .title {
  font-size: 36rpx;
  color: #333333;
  font-weight: 600;
  padding: 40rpx;
  box-sizing: border-box;
}
.wallet_box .detail .btn {
  width: 169rpx;
  background: #F1F2F2;
  border-radius: 34rpx;
  font-size: 24rpx;
  color: #777777;
  text-align: center;
  line-height: 67rpx;
}
.wallet_box .detail .content {
  padding: 0 40rpx 40rpx;
  box-sizing: border-box;
}
.wallet_box .detail .content .item {
  padding: 24rpx 0;
  border-bottom: 1rpx solid #EBEBEB;
}
.wallet_box .detail .content .item .colorred {
  color: red;
}
.wallet_box .detail .content .item .right {
  font-size: 30rpx;
  color: #E40030;
}
.wallet_box .detail .content .item .item_left image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.wallet_box .detail .content .item .item_left .top {
  font-size: 32rpx;
  color: #333333;
}
.wallet_box .detail .content .item .item_left .bom {
  font-size: 24rpx;
  color: #777777;
}

